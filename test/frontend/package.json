    "axios": ">=1.12.2",
    "class-variance-authority": ">=0.7.1",
    "clsx": ">=2.1.1",
    "maska": ">=3.2.0",
    "pinia": ">=3.0.3",
    "tailwind-merge": ">=3.3.1",
    "tailwindcss": ">=4.1.13",
